program factorial;

func int factorial(int n)
{   var int i, res;
    i = 1
    res = 1
    while(i <= n) then {
        res = res * i
        i = i + 1
    };
    return res
}

int main()
{
    var int n;

    print("Enter a positive integer: ")
    read(n)
    /* n = 5 */

    print("The factorial is = ", {factorial(n)})
}