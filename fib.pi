program fib;

int main() {
    var int n, t1, t2, nextTerm;
    var int j;
    n = 12
    t1 = 0
    t2 = 1
    nextTerm = 0
    j = 1
    while (j <= n) then {
      if (j == 1) then {
        print(t1)
      };
      if (j == 2) then {
        print(t2)
      };
      nextTerm = t1 + t2
      t1 = t2
      t2 = nextTerm
      print(nextTerm)
      j = j + 1
    };
}