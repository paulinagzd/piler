program fibR;

func int fib(int x) {
  print(x)
   if ((x==1) || (x==0)) then {
      return (x)
   } else {
      return ( {fib(x - 1)} + {fib(x - 2)})
   };
}

int main() {
  var int x;
  var int i;
  i = 0
  print("Enter the number of terms of series : ")
  /*  cin >> x; */
  x = 4
  print("\nFibonnaci Series : ")
  while (i < x) then {
    print({fib(i)})
    i = i + 1
  };
}